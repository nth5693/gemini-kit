{
  "name": "gemini-kit",
  "version": "0.4.0",
  "description": "Gemini CLI-style AI Development Assistant powered by Google Gemini",
  "author": "hieu",
  "license": "MIT",
  "type": "module",
  "bin": {
    "gk": "./dist/cli/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "pnpm build"
  },
  "keywords": [
    "ai",
    "cli",
    "gemini",
    "google",
    "development",
    "assistant",
    "agents",
    "coding"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nth5693/gemini-kit.git"
  },
  "homepage": "https://github.com/nth5693/gemini-kit#readme",
  "bugs": {
    "url": "https://github.com/nth5693/gemini-kit/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/react": "^19.2.7",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "gradient-string": "^3.0.0",
    "ink": "^6.5.1",
    "inquirer": "^12.0.0",
    "ora": "^8.1.1",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cli-progress": "^3.11.6",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.49.0",
    "vitest": "^2.1.0"
  }
}